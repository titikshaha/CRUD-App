{
    "version": 2,
    "name": "crud",
    "builds": [
      {
        "src": "backend/api/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "frontend/build"
        }
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/backend/api/$1" },
      { "src": "/(.*)", "dest": "/frontend/$1" }
    ]
  }
  